-- Список игроков
PlayerList.Name = "PlayerList"
PlayerList.Parent = Main
PlayerList.BackgroundColor3 = Color3.fromRGB(0, 100, 0)
PlayerList.Position = UDim2.new(0.05, 0, 0.42, 0)
PlayerList.Size = UDim2.new(0.9, 0, 0.48, 0)
PlayerList.ScrollBarThickness = 10

PlayerListLayout.Parent = PlayerList
PlayerListLayout.SortOrder = Enum.SortOrder.LayoutOrder
PlayerListLayout.Padding = UDim.new(0, 5)

local function updatePlayerList()
    for _,v in ipairs(PlayerList:GetChildren()) do
        if v:IsA("TextButton") then
            v:Destroy()
        end
    end

    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
        if player ~= lp then
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(1, 0, 0, 30)
            button.Text = player.Name
            button.TextColor3 = Color3.fromRGB(0, 255, 0)
            button.BackgroundColor3 = Color3.fromRGB(0, 99, 0)
            button.Parent = PlayerList

            button.MouseButton1Click:Connect(function()
                TextBox.Text = player.Name
            end)
        end
    end
end

-- Обновляем список игроков
updatePlayerList()

TextButton.Parent = Main
TextButton.BackgroundColor3 = Color3.fromRGB(0, 99, 0)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.1, 0, 0.75, 0)
TextButton.Size = UDim2.new(0.8, 0, 0.1, 0)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "Fling player"
TextButton.TextColor3 = Color3.fromRGB(0, 255, 0)
TextButton.TextScaled = true
TextButton.TextSize = 14.000
TextButton.TextWrapped = true

TextButton.MouseButton1Click:Connect(function()
    local Target = gplr(TextBox.Text)
    if Target[1] then
        Target = Target[1]
        -- (А здесь ваш код для флинга игрока)
    else
        print("Игрок не найден")
    end
end)

local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 36, 0, 37)
closeButton.Position = UDim2.new(0.5, 113, 0.5, -94)
closeButton.BackgroundColor3 = Color3.new(0.8, 0, 0)
closeButton.Text = "X"
closeButton.TextColor3 = Color3.new(1, 1, 1)
closeButton.Parent = Main
closeButton.MouseButton1Click:Connect(function()
    h:Destroy()
end)

-- Обновление списка игроков при добавлении/удалении игрока
game:GetService("Players").PlayerAdded:Connect(updatePlayerList)
game:GetService("Players").PlayerRemoving:Connect(updatePlayerList)
